('');\n\n  // 发送请求\n  axios.get(url).then(response => {\n    // 改变加载状态\n    loading.value = false;\n    data.value = response.data;\n  }).catch(error => {\n    // 改变加载状态\n    loading.value = false;\n    errorMsg.value = error.message || '位置错误'\n  })\n  return {\n    loading,\n    data,\n    errorMsg\n  }\n}"],"sourceRoot":""}]}